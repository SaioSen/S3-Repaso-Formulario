@model List<SolicitudAdopcion>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Lista de solicitudes</h1>

    @if (Model.Count == 0) {
        <p>No hay solicitudes aún.</p>
    }


    @foreach (var solicitud in Model){
         <p>@solicitud.NombreCompleto - @solicitud.Direccion</p>
    }

    
</div>
